Public Sub Propostas_Encaminhamento_Sem_Tabela()

Set DOC = Word.Documents.Application.ActiveDocument
'("C:\Users\it131\OneDrive\Área de Trabalho\Desenvolvimento - 2ª Versão\Anexo IX - Contrato decorrente de ARP\Modelo_Anexo IX_Contrato decorrente de ARP.docx")
With DOC
       
'       Bloco Proposições
       
        If Sheets("prop.encaminhamento").Range("C12") <> "" Then
            .Application.Selection.GoTo What:=wdGoToHeading, Which:=wdGoToFirst
            .Application.Selection.Find.Text = "#PROP1"
            .Application.Selection.Find.Execute
            .Application.Selection.Range = Sheets("prop.encaminhamento").Range("C12")
        Else
            .Application.Selection.GoTo What:=wdGoToHeading, Which:=wdGoToFirst
            .Application.Selection.Find.Text = "#PROP1."
            .Application.Selection.Find.Execute
            .Application.Selection.Delete Unit:=wdCharacter, Count:=1
        End If
       
        If Sheets("prop.encaminhamento").Range("D12") <> "" Then
            .Application.Selection.GoTo What:=wdGoToHeading, Which:=wdGoToFirst
            .Application.Selection.Find.Text = "#PROPTEXTO1"
            .Application.Selection.Find.Execute
            .Application.Selection.Range = Sheets("prop.encaminhamento").Range("D12")
        Else
            .Application.Selection.GoTo What:=wdGoToHeading, Which:=wdGoToFirst
            .Application.Selection.Find.Text = "#PROPTEXTO1"
            .Application.Selection.Find.Execute
            .Application.Selection.Delete Unit:=wdCharacter, Count:=2
        End If
                
        If Sheets("prop.encaminhamento").Range("C14") <> "" Then
            .Application.Selection.GoTo What:=wdGoToHeading, Which:=wdGoToFirst
