Public Sub Propostas_Encaminhamento()

tabela = 5

Set DOC = Word.Documents.Application.ActiveDocument
'("C:\Users\it131\OneDrive\Área de Trabalho\Desenvolvimento - 2ª Versão\Anexo IX - Contrato decorrente de ARP\Modelo_Anexo IX_Contrato decorrente de ARP.docx")
With DOC

'       Bloco Proposta de Encaminhamento 1
        
        If Sheets("prop.encaminhamento").Range("C12") = "Citação" Then
                If Sheets("Citação").Range("E11") = "Sim" Then
                    .Application.Selection.GoTo What:=wdGoToHeading, Which:=wdGoToFirst
                    .Application.Selection.Find.Text = "#DESTCIT1"
                    .Application.Selection.Find.Execute
                    .Application.Selection.Range = Sheets("Citação").Range("E13")
                    
                    .Application.Selection.GoTo What:=wdGoToHeading, Which:=wdGoToFirst
                    .Application.Selection.Find.Text = "#CARGOCIT1"
                    .Application.Selection.Find.Execute
                    .Application.Selection.Range = Sheets("Citação").Range("E14")
                    
                    .Application.Selection.GoTo What:=wdGoToHeading, Which:=wdGoToFirst
                    .Application.Selection.Find.Text = "#CPFCNPJCIT1"
                    .Application.Selection.Find.Execute
                    .Application.Selection.Range = Sheets("Citação").Range("E15")
                    
                    .Application.Selection.GoTo What:=wdGoToHeading, Which:=wdGoToFirst
                    .Application.Selection.Find.Text = "#IRREGIDENTCIT1"
                    .Application.Selection.Find.Execute
                    .Application.Selection.Range = Sheets("Citação").Range("E16")
                    
                    .Application.Selection.GoTo What:=wdGoToHeading, Which:=wdGoToFirst
                    .Application.Selection.Find.Text = "#CONDUTAIRREGCIT1"
                    .Application.Selection.Find.Execute
                    .Application.Selection.Range = Sheets("Citação").Range("E17")
                    
                    .Application.Selection.GoTo What:=wdGoToHeading, Which:=wdGoToFirst
                    .Application.Selection.Find.Text = "#DATAIRREGCIT1"
                    .Application.Selection.Find.Execute
                    .Application.Selection.Range = Sheets("Citação").Range("E18")
                    
                    .Application.Selection.GoTo What:=wdGoToHeading, Which:=wdGoToFirst
                    .Application.Selection.Find.Text = "#CONDUTAESPCIT1"
                    .Application.Selection.Find.Execute
                    .Application.Selection.Range = Sheets("Citação").Range("E19")
                    
                    .Application.Selection.GoTo What:=wdGoToHeading, Which:=wdGoToFirst
                    .Application.Selection.Find.Text = "#FUNDAMENTOJURIDICO1"
                    .Application.Selection.Find.Execute
                    .Application.Selection.Range = Sheets("Citação").Range("E20")
                    
                    tabela = tabela + 1
               Else
                    .Application.Selection.GoTo What:=wdGoToHeading, Which:=wdGoToFirst
                    .Tables(tabela).Select
                    .Tables(tabela).Delete
                    .Application.Selection.Delete Unit:=wdCharacter, Count:=1
                End If
            End If
            
